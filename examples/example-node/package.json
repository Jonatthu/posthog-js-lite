{
  "name": "example-node",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "preinstall": "yarn build:posthog",
    "prestart": "yarn build:posthog",
    "start": "ts-node server.ts",
    "build:posthog": "yarn build:posthog:publish && yarn build:posthog:link",
    "build:posthog:publish": "cd ../../ && yarn build && cd posthog-node && yalc publish",
    "build:posthog:link": "yalc add posthog-node"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "posthog-node": "file:.yalc/posthog-node"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.3",
    "eslint": "^8.19.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
